#include <bits/stdc++.h>
using namespace std;
int main()
{
    ios::sync_with_stdio(false);
    int n,m,b[1005];
    string str,str1;
    vector <int> a[1005];
    float p[1005],q[1005];
    cin>>n>>m;
    fill(p,p+n,0);
    fill(q,q+n,0);
    for(i=0;i<m;i++)
    {
        cin>>x;
        a[i].push_back(x);
        for(j=0;j<a[i][0];j++)
        {
            cin>>str;
            if(str[0]!='-')
            {
                b[str[0]-'0']=1;
                a[i].push_back(str[0]-'0');
            }
            else
            {
                b[str[1]-'0']=0;
                a[i].push_back(str[1]-'0');
            }
        }
    }
    cin>>str>>str1;
    if(str[0]!='-')
    {
        x=str[0]-'0';
        y=str1[1]-'0';
    }
    else
    {
        x=str[1]-'0';
        y=str1[0]-'0';
    }
    for(i=0;i<m;i++)
        []
}
